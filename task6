package HW7;

public class hw7ArrayD2 {

    public static void main(String[] args) {

      //  From an array of integer elements find the largest number.


       int[] num={12,-45,1,3,65,2,31,11,44,5,6};

 int max= num[0] ;

        for (int i = 0; i < num.length; i++) {

            if(num[i]>max) {
                max=num[i];
                System.out.println(max);
            }


        }










    }
}
